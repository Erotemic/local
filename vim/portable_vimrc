:set runtimepath+=~/local/vim/vimfiles/

python << endpython
import vim
enabled = [
    'jedi-vim',
    'nerdcommenter',
    'nerdtree',
    'supertab',
    'syntastic',
    'vim-startify',
] 

# Add a plugin to this list to disable it 
disabled = [
    'csv.vim',
    'grep.vim',
    'vim-repeat',
    'Conque-Shell',
    'highlight.vim',
    'vim-airline',
    'vim-multiple-cursors',
    'vim-sensible',
    'vim-surround',
    'vim-unimpaired',
    'vim-vimtweak',
]
vim.command('let g:pathogen_disabled = ' + repr(disabled))
endpython

