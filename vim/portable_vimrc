import vim
enabled = [
    'jedi-vim',
    'nerdcommenter',
    'nerdtree',
    'syntastic',
    'vim-startify',
] 

# Add a plugin to this list to disable it 
disabled = [
    'supertab',
    'vim-quickopen-tvio',
    'taglist.vim',
    'vim-fugitive'
    'csv.vim',
    'grep.vim',
    'vim-repeat',
    'Conque-Shell',
    'highlight.vim',
    'vim-airline',
    'vim-multiple-cursors',
    'vim-sensible',
    'vim-surround',
    'vim-unimpaired',
    'vim-vimtweak',
]
vim.command('let g:pathogen_disabled = ' + repr(disabled))
endpython

